{
  "name": "@ronas-it/figma-export",
  "version": "0.3.0",
  "description": "A CLI utility for exporting data from Ronas IT Figma designs for frontend use",
  "main": "src/figma-export.js",
  "bin": {
    "figma-export": "./src/figma-export.js",
    "figma-markup": "./src/markup.js"
  },
  "keywords": [
    "figma",
    "export",
    "cli",
    "scss",
    "react",
    "jsx",
    "design-tokens",
    "css-variables",
    "frontend",
    "design-system"
  ],
  "author": "Ronas IT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ronasit/ronasit-figma-export.git"
  },
  "homepage": "https://github.com/ronasit/ronasit-figma-export#readme",
  "bugs": {
    "url": "https://github.com/ronasit/ronasit-figma-export/issues"
  },
  "files": [
    "src/",
    "README.md",
    ".env.example"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky",
    "release": "npm publish"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "commander": "^9.5.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
